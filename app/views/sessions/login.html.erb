<h1>Log In</h1>

<%= form_with scope: :session, url: login_path do |form| %>

    <div>
        <%= form.label :email %>
        <%= form.email_field :email %>
    </div>

    <div>
        <%= form.label :password %>
        <%= form.password_field :password %>
    </div>

    <%= form.button "Log in" %>
<%end%>